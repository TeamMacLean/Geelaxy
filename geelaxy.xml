<?xml version="1.0"?>
<tool id="geelaxy" name="From GeeFu.io" workflow_compatible="false">
    <description>import data from geefu.io</description>
    <command interpreter="puthon">geelaxy.py</command>

    <inputs>
	<param name="path" type="string" label="username/organism/genome/experiment" help="example: wookoouk/Ash/KW1/exp1" />
    </inputs>

    <help>
	Currently only supports public data
    </help>

</tool>
